{
  "name": "Full Page Screen Capture",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Screen capture your current page in entirety and reliably!",
  "browser_action": {
    "default_icon": "icon-999.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs","activeTab","storage"    
  ],
  "icons": {
    "16": "icon16-999.png",
    "48": "icon48-999.png",
    "128": "icon128-999.png"
  },
  "options_page": "options.html"
}
